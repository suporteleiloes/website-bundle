{
    "name": "suporteleiloes/website-bundle",
    "description": "Bundle para website de leiloeiros da Suporte Leil√µes",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tiago Felipe",
            "email": "tiago@tiagofelipe.com"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "symfony/config": "^5.2",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/security-bundle": "^5.2",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/form": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/cache": "^5.2",
        "symfony/mailer": "^5.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/browser-kit": "^5.2",
        "symfony/console": "^5.2",
        "symfony/dom-crawler": "^5.2",
        "symfony/var-dumper": "^5.2",
        "symfony/yaml": "^5.2",
        "doctrine/doctrine-fixtures-bundle": "^3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SL\\WebsiteBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SL\\WebsiteBundle\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
